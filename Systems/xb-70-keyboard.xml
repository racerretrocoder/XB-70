<PropertyList>

  <keyboard>

    <key n="100">
      <name>d</name>
      <desc>door</desc>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </key>

    <key n="68">
      <name>D</name>
      <desc>bay</desc>
      <binding>
        <command>nasal</command>
        <script>doors.passenger.toggle();</script>
      </binding>
    </key>
