    <key n="69">
      <name>shift+e (E)</name>
      <desc>Drop bomb</desc>
      <binding>
        <condition>
          <!-- <property>sim/model/door-positions/passenger/position-norm</property> -->
          <property>sim/multiplay/generic/float[1]</property>
        </condition>
        <command>property-assign</command>
        <!-- <property>controls/armament/trigger</property> -->
        <property>sim/multiplay/generic/int[10]</property>
        <value>true</value>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <!-- <property>controls/armament/trigger</property> -->
          <property>sim/multiplay/generic/int[10]</property>
          <value>false</value>
        </binding>
      </mod-up>
    </key>

    <key n="81">
      <name>Q</name>
      <desc>Reset view</desc>
      <binding>
        <command>nasal</command>
        <script>
          setprop("sim/current-view/field-of-view",      getprop("sim/current-view/config/default-field-of-view-deg"));
          setprop("sim/current-view/heading-offset-deg", getprop("sim/current-view/config/heading-offset-deg"));
          setprop("sim/current-view/pitch-offset-deg",   getprop("sim/current-view/config/pitch-offset-deg"));
          setprop("sim/current-view/roll-offset-deg",    getprop("sim/current-view/config/roll-offset-deg"));
        </script>
      </binding>
    </key>

    <key n="125">
      <name>}</name>
      <desc>Autostart/AutoStop</desc>
      <binding>
        <command>property-toggle</command>
        <property>controls/electric/battery-switch</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/electric/engine/generator</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/instrument-lights</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/nav-lights</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/beacon</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/strobe</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/starterkey-insert</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/fuel-pump</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[1]/fuel-pump</property>
      </binding>
      <!-- Magnetos ON -->
      <binding>
        <condition>
          <property>controls/switches/starterkey-insert</property>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[0]/magnetos</property>
        <value>3</value>
      </binding>
      <binding>
        <condition>
          <property>controls/switches/starterkey-insert</property>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[1]/magnetos</property>
        <value>3</value>
      </binding>
      <binding>
        <condition>
          <property>controls/switches/starterkey-insert</property>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[0]/selected</property>
        <value>true</value>
      </binding>
      <binding>
        <condition>
          <property>controls/switches/starterkey-insert</property>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[1]/selected</property>
        <value>true</value>
      </binding>
      <binding>
        <condition>
          <property>controls/switches/starterkey-insert</property>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[2]/selected</property>
        <value>true</value>
      </binding>
      <binding>
        <condition>
          <property>controls/switches/starterkey-insert</property>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[3]/selected</property>
        <value>true</value>
      </binding>
      <!-- Magnetos OFF -->
      <binding>
        <condition>
          <not><property>controls/switches/starterkey-insert</property></not>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[0]/magnetos</property>
        <value>0</value>
      </binding>
      <binding>
        <condition>
          <not><property>controls/switches/starterkey-insert</property></not>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[1]/magnetos</property>
        <value>0</value>
      </binding>
      <binding>
        <condition>
          <not><property>controls/switches/starterkey-insert</property></not>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[0]/selected</property>
        <value>false</value>
      </binding>
      <binding>
        <condition>
          <not><property>controls/switches/starterkey-insert</property></not>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[1]/selected</property>
        <value>false</value>
      </binding>
      <binding>
        <condition>
          <not><property>controls/switches/starterkey-insert</property></not>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[2]/selected</property>
        <value>false</value>
      </binding>
      <binding>
        <condition>
          <not><property>controls/switches/starterkey-insert</property></not>
        </condition>
        <command>property-assign</command>
        <property>consumables/fuel/tank[3]/selected</property>
        <value>false</value>
      </binding>
      <!-- ******************************************* -->
      <binding>
        <command>nasal</command>
        <script>engines.autostart();</script>
      </binding>
    </key>
 <key n="108">
	 <name>l</name>
     <desc>Trigger flare release</desc>      
	 <binding>
     <command>nasal</command>
 <script>CMS.updatecms();CMS.trigger();</script>
      </binding>
  <binding>
   <command>property-toggle</command>
   <property>controls/flare</property>
  </binding>

    </key>
	 <key n="114">
  <name>r</name>
  <desc>Switch Radar Range</desc>
  <binding>
 <command>nasal</command>
 <script>radar.switch_distance();screen.log.write("Radar Range: " ~ f22.myRadar.rangeTab[radar.rangeIndex] ~ "nm.",1,1,0);</script>
</binding>
 </key> 
<key n="121">
  <name>y</name>
  <desc>select target</desc>
  <binding>
 <command>nasal</command>
 <script>radar.next_Target_Index();</script>
</binding>

 </key> 
<key n="101">
  <name>e</name>
  <desc>Fire selected WCS weapon</desc>
	<binding>
		<command>nasal</command>
		<script>setprop("controls/armament/trigger",1);#guns.fire_MG();</script>
	</binding>
    <binding>
   <command>property-toggle</command>
   <property>controls/missile</property>
  </binding>

  <mod-up>
    <binding>
      <command>nasal</command>
      <script>setprop("controls/armament/trigger",0);guns.stopFiring();</script>
    </binding>
    <binding>
      <command>nasal</command>
      <script>setprop("/controls/armament/missile-trigger", 0);</script>
    </binding>
  </mod-up>
 </key>

 <key n="109">
  <name>m</name>
  <desc>Cycle selected WCS AA weapon</desc>
  <binding>
 <command>property-cycle</command>
 <property>controls/armament/selected-weapon</property>
 <value>AGM-158</value>
 <value>AGM-154</value>
 <value>JDAM</value>
 <value>B61</value>
</binding>
  <binding>
 <command>property-cycle</command>
 <property>controls/armament/selected-weapon-digit</property>
 <value>1</value>
 <value>2</value>
 <value>4</value>
</binding>
  <binding><command>nasal</command><script>screen.log.write("Selected " ~ getprop("controls/armament/selected-weapon"),0.5,0.5,1);</script></binding>
   <binding><command>nasal</command><script>setprop("/controls/baydoors/AIM120", 0)</script></binding>
      <binding><command>nasal</command><script>setprop("/controls/missile", 3)</script></binding>
 </key> 
<key n="119">
  <name>w</name>
  <desc>Cycle Gun/Missile</desc>
  <binding>
 <command>property-cycle</command>
 <property>controls/armament/stick-selector</property> <!--Working!! :D-->
 <value>1</value>
 <value>2</value>
</binding>
 </key> 	

  </keyboard>

</PropertyList>
