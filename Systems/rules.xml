<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <autopilot>
    <path>Aircraft/XB-70/Systems/autopilot.xml</path>
  </autopilot>

  <property-rule n="100">
    <path>Aircraft/XB-70/Systems/glass-rain.xml</path>
  </property-rule>

  <pitot>
    <name>pitot</name>
    <number>0</number>
  </pitot>

  <static>
    <name>static</name>
    <number>0</number>
    <tau>1</tau>
  </static>

  <vacuum>
    <name>vacuum</name>
    <number>0</number>
    <rpm>/engines/engine[0]/rpm</rpm>
    <scale>1.0</scale>
  </vacuum>

</PropertyList>
