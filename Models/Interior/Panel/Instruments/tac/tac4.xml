<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>tac.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>needle</object-name>
    <object-name>needle2</object-name>
    <object-name>face</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>fond</object-name>
    <object-name>needle</object-name>
    <object-name>needle2</object-name>
    <object-name>face</object-name>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Compositor -->
  <effect>
    <inherits-from>../../../../Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>needle2</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.40 </green>
      <blue>  0.30 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <condition>
      <!-- <not><property>controls/engines/engine[4]/cutoff</property></not> -->
      <not><property>sim/multiplay/generic/bool[21]</property></not>
    </condition>
    <property>engines/engine[4]/n2</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>    0 </dep></entry>
      <entry><ind>  50 </ind><dep> -230 </dep></entry>
      <entry><ind> 100 </ind><dep>  270 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!--Rotation center of needle2
