    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>surface-positions/flap-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
    </flap>

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.0003</factor>
        <!--min>0.03</min-->
        <max>0.05</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>

    <stall>
      <name>stall</name>
      <path>Sounds/stall.wav</path>
      <property>sim/alarms/stall-warning</property>
      <volume>
        <property>velocities/airspeed-kt</property>
        <min>90.0</min>
      </volume>
    </stall>

    <crash>
      <name>intense ground contact</name>
      <mode>transient</mode>
      <path>Sounds/crash.wav</path>
      <property>sim/crashed</property>
      <volume>
        <factor>0.3</factor>
      </volume>
      <pitch>
        <factor>0.5</factor>
      </pitch>
    </crash>

    <door>
      <name>Crew door</name>
      <path>Sounds/door_close.wav</path>
      <condition>
        <equals>
          <!-- <property>instrumentation/doors/crew/position-norm</property> -->
          <property>sim/multiplay/generic/float[0]</property>
          <value>0.0</value>
        </equals>
      </condition>
      <volume>
        <factor>0.4</factor>
      </volume>
    </door>

    <door>
      <name>Passenger door</name>
      <path>Sounds/door_close.wav</path>
      <condition>
        <equals>
          <!-- <property>instrumentation/doors/passenger/position-norm</property> -->
          <property>sim/multiplay/generic/float[1]</property>
          <value>0.0</value>
        </equals>
      </condition>
      <volume>
        <factor>0.4</factor>
      </volume>
    </door>

    <rain>
      <name>rain</name>
      <mode>looped</mode>
      <path>rain_canopy.wav</path>
      <condition>
        <greater-than>
          <property>environment/metar/rain-norm</property>
          <value>0.2</value>
        </greater-than>
      </condition>
      <volume>
        <property>environment/metar/rain-norm</property>
        <factor>1.00</factor>
        <offset>0.00</offset>
        <min>0.4</min>
        <max>0.9</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.001</factor>
        <offset>1.0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rain>

  </fx>

</PropertyList>
