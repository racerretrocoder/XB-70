<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <fx>

    <!-- Engine 1 -->
    <engine>
      <name>start1</name>
      <mode>once</mode>
      <path>starter.wav</path>
      <condition>
        <property>controls/engines/engine[0]/starter</property>
      </condition>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        <min> 0.0 </min>
        <max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine-jet>
      <name>engine-jet1</name>
      <path>Sounds/jet.wav</path>
      <mode>looped</mode>
      <condition>
        <not><property>controls/engines/engine[0]/cutoff</property></not>
        <not><property>sim/current-view/internal</property></not>
        <not><property>engines/engine[0]/out-of-fuel</property></not>
      </condition>
      <orientation>
        <x>0.0</x>
        <y>0.0</y>
        <z>-180.0</z>
        <inner-angle>180.0</inner-angle>
        <outer-angle>360.0</outer-angle>
        <outer-gain>0.2</outer-gain>
      </orientation>
      <pitch>
        <property>engines/engine[0]/n1</property>
        <ofset>0.1</ofset>
        <factor>.001</factor>
      </pitch>
      <volume>
        <property>engines/engine[0]/thrust-lbs</property>
        <factor>0.000025</factor>
        <min>0.2</min>
        <max>0.75</max>
      </volume>
    </engine-jet>

    <engine-rumble>
      <name>afterburnerOUT1</name>
      <path>afterburner-out.wav</path>
      <mode>looped</mode>
      <condition>
        <and>
          <not><property>controls/engines/engine[0]/cutoff</property></not>
          <not><property>sim/current-view/internal</property></not>
          <not><property>engines/engine[0]/out-of-fuel</property></not>
          <greater-than>
            <property>engines/engine[0]/afterburner</property>
            <value>0.7</value>
          </greater-than>
        </and>
      </condition>
      <orientation>
        <x>0.0</x>
        <y>0.0</y>
        <z>-180.0</z>
        <inner-angle>180.0</inner-angle>
        <outer-angle>360.0</outer-angle>
        <outer-gain>0.2</outer-gain>
      </orientation>
      <pitch>
        <property>engines/engine[0]/afterburner</property>
        <factor>1</factor>
        <offset>0.3</offset>
      </pitch>
      <volume>
        <property>engines/engine[0]/afterburner</property>
        <!--<factor>0.7</factor>-->
        <factor>1.2</factor>
        <offset>0.15</offset>
        <!--<max>1</max>-->
      </volume>
    </engine-rumble>

    <engine-rumble>
